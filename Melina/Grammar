program = suites | subscenarioDefinitions | program
subscenarioDefinitions = "subscenario" STRING ":" steps "end"
suites = suite (suite)*
suite = "suite" STRING ":" arguments? scenarios "end"
arguments = "arguments" ":" argumentsBody "end"
argumentsBody = argument (argument)*
argument = STRING "to" STRING
scenarios = scenario (scenario)*
scenario = "scenario" STRING ":" steps "end"
steps = step (steps)*
step = action | subscenarioCall
action = actionType element condition
actionType = "verify" | "tap" | "edit"
element = elementType STRING
elementType = "button" | "view" | "textfield" | "label"
condition = selected | exist | contains | text
selected = "is not selected" | "is selected"
exist = "is not exist" | "is exist"
contains = "contains value" STRING
text = "with text" STRING
subscenarioCall = "subscenario" STRING
