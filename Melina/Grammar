program = suite*
suite = "suite" arguments* STRING ":" scenario* "end"
arguments = "arguments" argumentsBody :  "end"
argumentsBody = argument (argument)*
argument = STRING ":" STRING
scenario = "scenario" STRING ":" steps* "end"
steps = step (steps)*
step = action element
action = actionType
actionType = "verify" | "tap" | "scrollUp" | "scrollDown"
element = elementType "[" "name" ":" STRING "]"
elementType = "button" | "screen" | "text" | "searchField"
