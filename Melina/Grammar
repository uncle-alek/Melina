program = suite*
suite = "suite" arguments* STRING ":" scenario* "end"
arguments = "arguments" argument* :  "end"
argument = STRING ":" STRING
scenario = "scenario" STRING ":" step* "end"
step = action STRING object
action = "verify" | "tap" | "scrollUp" | "scrollDown"
object = "button" | "screen" | "text" | "searchField"
